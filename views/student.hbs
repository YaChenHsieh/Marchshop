<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Age</th>
      <th scope="col">id</th>
    </tr>
  </thead>

  <tbody>
 {{#each info}}
    <tr>
      <th scope="row">1</th>
      <td>  {{this.name}} </td>
      <td>  {{this.age}}  </td>
      <td class="{{../myclass}}">  {{this.id}}   </td>
    </tr>
 {{/each}}
  </tbody>

</table>